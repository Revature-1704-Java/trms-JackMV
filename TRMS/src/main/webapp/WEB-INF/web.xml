<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RequestProcessorServlet</servlet-name>
  	<display-name>RequestProcessorServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controllers.RequestProcessorServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginRequestServlet</servlet-name>
  	<display-name>LoginRequestServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controllers.LoginRequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RequestProcessorServlet</servlet-name>
  	<url-pattern>/RequestProcessorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginRequestServlet</servlet-name>
  	<url-pattern>/LoginRequestServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>login.html</welcome-file>
  </welcome-file-list>
</web-app>
