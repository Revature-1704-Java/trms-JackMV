<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RequestProcessorServlet</servlet-name>
  	<display-name>RequestProcessorServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controllers.RequestProcessorServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginRequestServlet</servlet-name>
  	<display-name>LoginRequestServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controllers.LoginRequestServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SubmitReimbursementRequestServlet</servlet-name>
  	<display-name>SubmitReimbursementRequestServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controllers.SubmitReimbursementRequestServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewRequestsServlet</servlet-name>
  	<display-name>ViewRequestsServlet</display-name>
  	<servlet-class>com.revature.controllers.ViewRequestsServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ApproveRequestServlet</servlet-name>
  	<display-name>ApproveRequestServlet</display-name>
  	<servlet-class>com.revature.controllers.ApproveRequestServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RejectRequestServlet</servlet-name>
  	<display-name>RejectRequestServlet</display-name>
  	<servlet-class>com.revature.controllers.RejectRequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RequestProcessorServlet</servlet-name>
  	<url-pattern>/RequestProcessorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginRequestServlet</servlet-name>
  	<url-pattern>/LoginRequestServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SubmitReimbursementRequestServlet</servlet-name>
  	<url-pattern>/SubmitReimbursementRequestServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewRequestsServlet</servlet-name>
  	<url-pattern>/ViewRequestsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ApproveRequestServlet</servlet-name>
  	<url-pattern>/ApproveRequestServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RejectRequestServlet</servlet-name>
  	<url-pattern>/RejectRequestServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <error-page>
      <error-code>404</error-code>
      <location>/index.html</location>
  </error-page>
</web-app>